{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-dom"
    ],
    "baseUrl": ".",
    "outDir": "../../.moon/cache/types/packages/ui"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "./src/components/Icons/ExternalIcons/assets",
  ]
}
